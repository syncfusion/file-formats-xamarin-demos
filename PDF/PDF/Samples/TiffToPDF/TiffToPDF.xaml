<?xml version="1.0" encoding="utf-8" ?>
<local:SampleView
              xmlns:local="clr-namespace:SampleBrowser.Core;assembly=SampleBrowser.Core"
              xmlns="http://xamarin.com/schemas/2014/forms"
              xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
              x:Class="SampleBrowser.PDF.TiffToPDF">

    <local:SampleView.Content>
        <StackLayout Padding="10">

            <Label x:Name="Description" LineBreakMode="WordWrap" Text="This sample illustrates how to convert single or multi frame TIFF images to PDF." VerticalOptions="Center" FontSize="Medium" HorizontalOptions="Start">
            </Label>
            <Label x:Name="label2" Text=" " VerticalOptions="Center"></Label>
            <Button x:Name="btnGenerate"  Text="Convert to PDF" HorizontalOptions="Center" VerticalOptions="Center" Clicked="OnButtonClicked" />
        </StackLayout>
    </local:SampleView.Content>

    <local:SamplePage.PropertyView>

    </local:SamplePage.PropertyView>
</local:SampleView>